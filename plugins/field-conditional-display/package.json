  "keywords": [
    "kintone",
    "plugin",
    "field",
    "conditional",
    "display"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@kintone/dts-gen": "^9.0.0",
    "@kintone/plugin-packer": "^7.0.4",
