    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "posthog-js": "^1.313.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
