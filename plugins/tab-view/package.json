{
  "name": "kintone-plugin-tab-view",
  "version": "1.0.0",
  "description": "kintoneのレコード詳細画面をタブで整理するプラグイン",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "package": "kintone-plugin-packer --ppk private.ppk plugin",
    "upload": "kintone-plugin-uploader plugin.zip",
    "deploy": "npm run build && npm run package"
  },
  "keywords": [
    "kintone",
    "plugin",
    "tab",
    "ui",
    "レコード詳細"
  ],
  "author": "Sakaguchi Takaya",
  "license": "MIT",
  "devDependencies": {
    "@kintone/dts-gen": "^9.0.0",
    "@kintone/plugin-packer": "^9.0.0",
    "@kintone/plugin-uploader": "^10.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
